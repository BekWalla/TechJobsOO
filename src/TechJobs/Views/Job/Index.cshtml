@model TechJobs.ViewModels.JobFieldsViewModel.JobField

<table class="job-listing">
    @{ 
// TODO #2.2 - display the job fields, one per row (outside of this @{ } block)
    }
    <tr>
        <th>Name</th>
        <th>Employer</th>
        <th>Location</th>
        <th>Core Competency</th>
        <th>Position Type</th>
    </tr>

    @foreach (var job in Model)
    {
        <tr>
            <th>@job.Name</th>
            <th>@job.Employer</th>
            <th>@job.Location</th>
            <th>@job.CoreCompetency</th>
            <th>@job.PositionType</th>
        </tr>
    }
</table>